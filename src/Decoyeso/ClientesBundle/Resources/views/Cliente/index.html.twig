<h1>Cliente list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Telefono</th>
            <th>Celular</th>
            <th>Email</th>
            <th>Dni</th>
            <th>Cuitocuil</th>
            <th>Barrio</th>
            <th>Calle</th>
            <th>Numero</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('cliente_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.nombre }}</td>
            <td>{{ entity.apellido }}</td>
            <td>{{ entity.telefono }}</td>
            <td>{{ entity.celular }}</td>
            <td>{{ entity.email }}</td>
            <td>{{ entity.dni }}</td>
            <td>{{ entity.cuitOcuil }}</td>
            <td>{{ entity.barrio }}</td>
            <td>{{ entity.calle }}</td>
            <td>{{ entity.numero }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('cliente_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('cliente_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('cliente_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
